<!-- New form to sing up users -->
<h2>Sign up</h2>
<!-- F variable tells the called what the form is called -->
<%= simple_form_for @user do |f| %>

<%= f.input :username %>
<%= f.input :email %>
<%= f.input :password %>
<%= f.input :password_confirmation %>

<%= f.button :submit, "sign up" %>

<% end  %>
